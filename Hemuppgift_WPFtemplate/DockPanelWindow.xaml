<Window x:Class="Hemuppgift_WPFtemplate.DockPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockPanelWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Top" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="5" Width="80" Height="30"/>
        <Button Content="Left" HorizontalAlignment="Left" Margin="5" Width="80" Height="30"/>
        <Button Content="Center" HorizontalAlignment="Center" Margin="5" Width="80" Height="30" Click="Button_Click"/>
        <Button Content="Right" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="5" Width="80" Height="30"/>
        <Button Content="Bottom" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Width="80" Height="30"/>
        <Image x:Name="imageControl" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed" Width="200" Height="200"/>
    </Grid>
</Window>
